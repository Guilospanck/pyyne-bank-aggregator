{
  "name": "pyyne-bank-aggregator",
  "version": "1.0.0",
  "main": "src/main.ts",
  "repository": "https://github.com/Guilospanck/pyyne-bank-aggregator.git",
  "author": "Guilherme Rodrigues <guilherme.rp2014@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start:dev": "NODE_ENV=development yarn nodemon -r tsconfig-paths/register src/main.ts",
    "start": "NODE_ENV=production node dist/main.js",
    "build": "npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.js\" --source-maps inline",
    "test": "NODE_ENV=development jest --passWithNoTests --noStackTrace --runInBand --detectOpenHandles",
    "test:staged": "NODE_ENV=development yarn test -- --findRelatedTests src/",
    "test:cov": "NODE_ENV=development jest --coverage --detectOpenHandles"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "eslint": "^8.17.0",
    "express": "^4.18.1",
    "pino": "^8.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.5",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.0",
    "@types/node": "^18.0.0",
    "@types/pino": "^7.0.5",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.27.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "lint-staged": "^13.0.1",
    "nodemon": "^2.0.16",
    "ts-jest": "^27.1.4",
    "prettier": "^2.7.1",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3"
  }
}
