{
  "name": "pyyne-bank-aggregator",
  "version": "1.0.0",
  "main": "src/main.ts",
  "repository": "https://github.com/Guilospanck/pyyne-bank-aggregator.git",
  "author": "Guilherme Rodrigues <guilherme.rp2014@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start:dev": "NODE_ENV=development yarn nodemon -r tsconfig-paths/register src/main.ts",
    "start": "NODE_ENV=production node dist/main.js",
    "build": "npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.js\" --source-maps inline",
    "test": "NODE_ENV=development jest --passWithNoTests --noStackTrace --runInBand --detectOpenHandles",
    "test:staged": "NODE_ENV=development yarn test -- --findRelatedTests src/",
    "test:cov": "NODE_ENV=development jest --coverage --detectOpenHandles"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "eslint": "^8.11.0",
    "express": "^4.17.3",
    "pino": "^7.9.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-typescript": "^7.15.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/pino": "^7.0.5",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  }
}
